{
	"name": "eyecatchup/GetGitStats",
	"description": "Give it a nice description!",
	"keywords": [],
	"homepage": "https://github.com/eyecatchup/GetGitStats",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"bin-dir": "bin",
		"process-timeout": 0
	},
	"require": {
		"php": "^5.5|^7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8",
		"phpspec/phpspec": "~2.0",
		"satooshi/php-coveralls": "dev-master",
		"fabpot/php-cs-fixer": "~1.0",
		"squizlabs/php_codesniffer": "~1.5",
		"phpdocumentor/phpdocumentor": "2.*",
		"scrutinizer/ocular": "~1"
	},
	"autoload": {
		"psr-4": {
			"Eyecatchup\\GetGitStats\\": "src/Eyecatchup/GetGitStats"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Eyecatchup\\GetGitStats\\Tests\\": "tests/Eyecatchup/GetGitStats/Tests"
		}
	},
	"bin": [
		"vendor/bin/coveralls",
		"bin/coveralls"
	],
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
